{
  "name": "@datsfilipe/web-ascii-theater",
  "publishConfig": {
    "registry": "https://npm.pkg.github.com/datsfilipe"
  },
  "description": "Play ASCII animations on the web.",
  "repository": {
    "type": "git",
    "url": "https://github.com/datsfilipe/web-ascii-theater.git"
  },
  "bugs": {
    "url": "https://github.com/datsfilipe/web-ascii-theater/issues"
  },
  "version": "1.2.0",
  "type": "module",
  "scripts": {
    "dev": "node server",
    "build:preview": "npm run build:client && npm run build:server",
    "build:client": "vite build --outDir build/client",
    "build:server": "vite build --ssr src/entry-server.tsx --outDir build/server",
    "serve": "NODE_ENV=production node server",
    "ts-types": "tsc --emitDeclarationOnly --outDir dist",
    "build": "rimraf dist && node ./esbuild.mjs && npm run ts-types",
    "prepublishOnly": "npm run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [
    "badapple",
    "ascii",
    "ascii-art",
    "react",
    "animation"
  ],
  "author": "datsfilipe",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^18.0.21",
    "@types/react-dom": "^18.0.6",
    "@typescript-eslint/eslint-plugin": "^5.40.1",
    "@typescript-eslint/parser": "^5.40.1",
    "@vitejs/plugin-react": "^4.0.0",
    "compression": "^1.7.4",
    "esbuild": "^0.15.12",
    "esbuild-node-externals": "^1.5.0",
    "eslint": "^8.26.0",
    "eslint-plugin-react": "^7.31.10",
    "express": "^4.18.2",
    "ffmpeg-extract-frames": "^2.0.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "rimraf": "^3.0.2",
    "serve-static": "^1.15.0",
    "typescript": "^4.8.4",
    "vite": "^4.3.5",
    "youtube-dl-exec": "^2.2.3"
  },
  "peerDependencies": {
    "react": "^18.2.0"
  },
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts"
}
